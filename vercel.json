{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/.well-known/apple-app-site-association",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/algemene-voorwaarden",
      "dest": "/algemene-voorwaarden.html"
    },
    {
      "src": "/privacybeleid",
      "dest": "/privacybeleid.html"
    },
    {
      "src": "/auth/callback",
      "dest": "/auth/callback.html"
    },

    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 